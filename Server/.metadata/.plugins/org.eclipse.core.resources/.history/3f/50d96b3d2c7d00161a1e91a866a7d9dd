package Game;

import Server.GameServer;

public class TetrisGame implements Runnable {
	private GameServer gameServer;
	boolean gameRunning;
	
	public TetrisGame(GameServer gameServer)
	{
		this.gameServer = gameServer;
	}
	
	public void start()
	{
		run();
	}

	@Override
	public void run() 
	{
		while(gameRunning)
		{
			
		}		
	}
	
	
}
